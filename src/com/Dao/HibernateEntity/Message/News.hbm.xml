<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.Dao.HibernateEntity.Message">
    <class name="com.Dao.HibernateEntity.Message.News" table="t_news">
        <id name="news_Id" column="news_Id">
            <generator class="native"/>
        </id>
        <property name="news_addDate" column="news_addDate"/>
        <property name="news_display" column="news_display"/>
        <property name="news_Text" column="news_Text"/>
        <property name="news_Title" column="news_Title"/>
    </class>
</hibernate-mapping>