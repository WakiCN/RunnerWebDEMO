<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.Dao.HibernateEntity.Message">
    <class name="com.Dao.HibernateEntity.Message.News_Message" table="t_news_message">
        <id name="message_Id" column="message_id">
            <generator class="native"/>
        </id>
        <property name="news_Id" column="news_Id"/>
        <property name="id" column="id"/>
        <property name="message_Date" column="message_Date"/>
        <property name="message_text" column="message_text"/>
        <property name="message_display" column="message_display"/>
    </class>
</hibernate-mapping>